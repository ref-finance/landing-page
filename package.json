    "react-spring": "^9.4.2",
    "use-animate-number": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@parcel/transformer-sass": "^2.0.0",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.3.7",
